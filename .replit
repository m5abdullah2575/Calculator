modules = ["web", "nodejs-20", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Web Server"

[[workflows.workflow]]
name = "Web Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m http.server 5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 32825
externalPort = 3002

[[ports]]
localPort = 36219
externalPort = 3000

[[ports]]
localPort = 42301
externalPort = 4200

[[ports]]
localPort = 43197
externalPort = 3001

[[ports]]
localPort = 43199
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["python3", "-m", "http.server", "5000"]
